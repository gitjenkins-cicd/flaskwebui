run images for test purpose
docker run -d -p 4200:3000 --env-file=env --env-file=prisma/env regressionbackend-v1:latest

docker run -d -p 8080:8080 --env-file=env regressionfrontend-v1:latest

build command
 docker build -t regressionbackend-prisma-v1 .
